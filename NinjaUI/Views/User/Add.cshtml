@model NinjaUI.Models.UserDto

<h1 class="mt-3">Add Region</h1>

<form method="post">

    <div class="mt-3">
        <label class="form-label">Name</label>
        <input type="text" class="form-control" asp-for="Name" />
    </div>

    <div class="mt-3">
        <label class="form-label">Age</label>
        <input type="text" class="form-control" asp-for="Age" />
    </div>

    <div class="mt-3">
        <label class="form-label">Gender</label>
        <input type="text" class="form-control" asp-for="Gender" />
    </div>
    <div class="mt-3">
        <label class="form-label">Email</label>
        <input type="text" class="form-control" asp-for="Email" />
    </div>
    <div class="mt-3">
        <label class="form-label">PhoneNumber</label>
        <input type="text" class="form-control" asp-for="PhoneNumber" />
    </div>
    <div class="mt-3">
        <label class="form-label">Country</label>
        <input type="text" class="form-control" asp-for="Country" />
    </div>
    <div class="mt-3">
        <label class="form-label">City</label>
        <input type="text" class="form-control" asp-for="City" />
    </div>

    <div class="mt-3">
        <button type="submit" class="btn btn-primary">Save</button>
    </div>

</form>